{
  "BIOS_Memory":
  {
    "_type":"Memory.MappedMemory",
    "_connection":
    {
      "sysbus":
        [
           { "address":0x0 },
           { "address":0xFF0000, "size":0x10000 }, /* bios mirror 1 */
           { "address":0xFFFF0000, "size":0x10000 }, /* bios mirror 2 */
        ]
    },
   "size":0xFF0000,
  },

  "SDRAM_Memory":
  {
    "_type":"Memory.MappedMemory",
    "_connection":
    {
      "sysbus":
        [
            { "address":0x1000000 },
        ]
    },
    "size":0x20000000,
  },

  "uart":
  {
    "_type":"UART.NS16550",
   "_connection":
   {
       "sysbus":
       [
           { "address": 0xE00003F8 }
       ]
   },
  },
  "cpu":
  {
    "_type":"CPU.X86",
    "_connection":"sysbus",
    "cpuType":"x86",
    "lapic":"lapic"
  },
  "lapic":
  {
      "_type":"IRQControllers.LAPIC",
      "_connection":
      {
          "sysbus":
          [
              {"address":0xFEE00000},
          ]
      },
      "_irq":
      {
          "cpu":["IRQ", 0],
      }
  },
}
